\par \pard\plain \s0\nowidctlpar{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\aspalpha\ltrpar\cf0\kerning1\hich\af10\langfe2052\dbch\af8\afs24\lang1081\loch\f3\fs24\lang1049{\listtext\pard\plain   3.\tab}\ilvl0\ls1 \li720\ri0\lin720\rin0\fi-360\qj{\b0\ab0\rtlch \ltrch\loch\lang1033
